8) i,n:integer;n:=5;i:=1;flag:bool;flag:=false;
   *[i<n/2 -> [[n mod i = 0 -> flag := true;i:=i+1 | n mod i != 0 -> i:i+1]]]

10) a,b,c,i:integer;a:=0;b:=1;i:=3;
    *[i<=n -> c := c a + b; a:=b;b:=c;i++]

9)
  i,sil:integer;i:=1;sil:=1;
