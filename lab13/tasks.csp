1) skip

2) *[true -> skip]

3) i:integer;sum,avg,(i:1...100)tab(i):real;read(100,tab);i:=1;sum:=0;
   *[i<=100 -> sum+=tab(i);i++]
    avg := sum/100;

4) i:integer;(i:1...100)tab(i):real;present:bool;present:=false;
  *[(i:1...100)tab(i) != key(i)+1 ->[tab(i) == key -> present:=true | tab(i) != key -> skip];tab(i):=key+1]

5) i,j,N:integer;i:=1;j:=N;
  *[i<j -> (tab(i),tab(j):=tab(j),tab(i));i:=i+1;j:=j-1]

